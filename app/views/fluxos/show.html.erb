<h1><%= @fluxo %></h1>
<%= show_for @fluxo do |s| %>
  <%= s.attribute :nome %>
  <%= s.attribute :caso_de_uso_id %>

<% end %>
<div class="form-actions">
	<%= link_to 'Editar', edit_fluxo_path(@fluxo), :class => 'btn' %>
	<%= link_to 'Novo', new_fluxo_path, :class => 'btn' %>
	<%= link_to 'Ver lista', fluxos_path, :class => 'btn' %>
</div>
